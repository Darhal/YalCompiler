.data
	div_by0: .asciiz "[RUNTIME ERROR]: Division by zero detected."
.text

main:
	# Begin stackframe:
	move $s7, $sp

	# Integer Constant '5':
	li $v0, 5
	addi $sp, $sp, -4
	sw $v0, 0($sp)

	# Integer Constant '0':
	li $v0, 0
	lw $t8, 0($sp)
	addi $sp, $sp, 4
	# Division:
# test si on divise par 0
beqz $v0, div_by_zero
	div $t8, $v0
	mflo $v0

	# Call write sys call:â€‹
	move $a0, $v0
	li $v0, 1
	syscall
	# Return to line:
	li $v0, 11 	# Syscall code for printing one char
	li $a0, '\n' 	# print new line char
	syscall

	# Handle program exit
exit:
	li $v0, 10
	syscall

	 # Handle RUNTIME errors
div_by_zero:
	la $a0, div_by0
	li $v0, 4
	syscall

