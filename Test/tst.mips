.data
.text

main:
	# Begin stackframe:
	move $s7, $sp
	# Allocate for the declared variables:
	addi $sp, $sp, -4

	# Reads an integer: 
	li $v0, 5
	syscall
	sw $v0, 0($sp)

	# Get value of the variable 'a':
	lw $v0, 0($sp)
	# Branch Instruction:
	beq $v0, $zero, end_loop_855
start_loop_855:
	# Loop instructions:
	# Get value of the variable 'a':
	lw $v0, 0($sp)

	# Call write sys call:â€‹
	move $a0, $v0
	li $v0, 1
	syscall
	# Return to line:
	li $v0, 11 	# Syscall code for printing one char
	li $a0, '\n' 	# print new line char
	syscall
	bne $v0, $zero, start_loop_855
end_loop_855:

