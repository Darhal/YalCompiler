.data
	div_by0: .asciiz "[RUNTIME ERROR]: Division by zero detected."
	true_str: .asciiz "vrai"
	false_str: .asciiz "faux"
.text

main:
	# Begin stackframe:
	move $s7, $sp

	# Integer Constant '6':
	li $v0, 6
	addi $sp, $sp, -4
	sw $v0, 0($sp)

	# Integer Constant '5':
	li $v0, 5
	lw $t8, 0($sp)
	addi $sp, $sp, 4
	# Less:
	slt $v0, $t8, $v0

	 # Evalue if the expression is true or false
	beq $v0, $zero, wfalse_9590
wtrue_9590:
	la $a0, true_str
	li $v0, 4
	syscall
	j wend_9590
wfalse_9590:
	la $a0, false_str
	li $v0, 4
	syscall
wend_9590:
	# Handle program exit
exit:
	li $v0, 10
	syscall

	 # Handle RUNTIME errors
div_by_zero:
	la $a0, div_by0
	li $v0, 4
	syscall

