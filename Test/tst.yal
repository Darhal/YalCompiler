// Program that calculate the fibonacci sequence using the recursive, iterative and dynamic programming way: (It might take a while to calculate the results)
programme fib_test debut
	fonction fib_rec(entier n) debut
      	si n == 1 ou n == 0 alors
			retourne n;
		finsi
	
		retourne fib_rec(n-1) + fib_rec(n-2);
	fin
	
	fonction fib_itr(entier n) debut
		entier i; entier j; entier k; entier t;
		
		si n == 0 alors
			retourne 0;
		finsi
		
		i = 0;
		j = 1;
		k = 1;
		
		tantque (k < n) repeter
			t = i + j;
			i = j;
			j = t;
			
			k = k + 1;
		fintantque
		
		retourne j;
	fin
	
	fonction fib_itr_dp(entier n) debut
		entier i; 
		entier[n + 2] f;
		
		f[0] = 0;
		f[1] = 1;
		i = 2;
		
		tantque (i < n ou i == n) repeter
			f[i] = f[i-1] + f[i-2];
			i = i + 1;
		fintantque
		
		retourne f[n];
	fin
	
	fonction fib_rec_dp(entier n) debut
		si (f[n] == 0) alors
			si (n < 1 ou n == 1) alors
				f[n] = n;
			sinon
				f[n] = fib_rec_dp(n - 1) + fib_rec_dp(n - 2);  
			finsi
		finsi
		
		retourne f[n];
	fin
	
	//// Global table declaration:
	entier[100] f;
	
	////////////////// Iterative //////////////////
	ecrire fib_itr(0) == 0 et fib_itr(1) == 1 et fib_itr(3) == 2 et fib_itr(5) == 5 et fib_itr(8) == 21 et fib_itr(10) == 55 et fib_itr(18) == 2584 et fib_itr(23) == 28657;
		
	////////////////// Iterative using DP //////////////////
	ecrire fib_itr_dp(0) == 0 et fib_itr_dp(1) == 1 et fib_itr_dp(3) == 2 et fib_itr_dp(5) == 5 et fib_itr_dp(8) == 21 et fib_itr_dp(10) == 55 et fib_itr_dp(18) == 2584 et fib_itr_dp(23) == 28657;
	
	////////////////// Recursive using DP //////////////////
	ecrire fib_rec_dp(0) == 0 et fib_rec_dp(1) == 1 et fib_rec_dp(3) == 2 et fib_rec_dp(5) == 5 et fib_rec_dp(8) == 21 et fib_rec_dp(10) == 55 et fib_rec_dp(18) == 2584 et fib_rec_dp(23) == 28657;
	
	////////////////// Recursive //////////////////
	ecrire fib_rec(0) == 0 et fib_rec(1) == 1 et fib_rec(3) == 2 et fib_rec(5) == 5 et fib_rec(8) == 21 et fib_rec(10) == 55 et fib_rec(18) == 2584 et fib_rec(23) == 28657;
fin
