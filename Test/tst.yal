// Output: 2 3 4 5 6
programme tab_in_tab debut
	entier[5] a;
	entier[6] b;
	entier i; entier j;
	
	fonction a(entier b) debut
		entier[5] a;
		a[0] = b;
		
		si a[0] == 1 alors
			retourne 1;
		finsi
		
		retourne 0; // obligatoire ici ?
	fin
	
	i = 0;
	
	tantque i < 5 repeter
		j = 0;
	
		tantque j < 5 repeter
			ecrire i;
			ecrire j;
			
			j = j + 1;
		fintantque
		
		i = i + 1;
	fintantque
fin
